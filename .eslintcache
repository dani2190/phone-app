 var(--mainWhite);\n    font-size: 1.4rem;\n    border-radius: 0.5rem 0 0 0;\n    transform: translate(100%, 100%);\n    transition: all 1s ease-in-out;\n}\n.img-container:hover .cart-btn{\n    transform: translate(0,0);\n}\n.cart-btn:hover{\n    color:var(--mainBlue);\n    cursor: pointer;\n}\n`;\n","/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Default.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Title.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/data.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/context.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Button.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Modal.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Cart/Cart.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Cart/CartColumns.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Cart/EmptyCart.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Cart/CartList.js",[],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Cart/CartItem.js",[],["89","90"],"/Users/danielgatabazi/Desktop/Mern/phone/phone-app/src/components/Cart/CartTotals.js",["91"],"import React, { Component } from \"react\";\nimport { Link } from \"react-router-dom\";\nexport default class CartTotals extends Component {\n    render() {\n        const {\n            cartSubTotal,\n            cartTax,\n            cartTotal,\n            cart,\n            clearCart\n        } = this.props.value;\n        const { history } = this.props;\n        const emptyCart = cart.length === 0 ? true : false;\n        return (\n            <React.Fragment>\n                {!emptyCart && (\n                    <div className=\"container\">\n                        <div className=\"row\">\n                            <div className=\"col-10 mt-2 ml-sm-5 ml-md-auto col-sm-8